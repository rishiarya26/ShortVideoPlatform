{
  "ci": {
    "collect": {
      "settings": {
        "chromeFlags": "--ignore-certificate-errors"
      },
      "startServerCommand": "npm run lhci:serve",
      "startServerReadyPattern": "Listening",
      "reportFilenamePattern": "%%HOSTNAME%%-%%PATHNAME%%-.report.%%EXTENSION%%",
      "url": [
        "https://127.0.0.1:8000",
        "https://127.0.0.1:8000/repos",
        "https://127.0.0.1:8000/repo-details/Homebrew/brew"
      ],
      "numberOfRuns": 1
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./perf/page-perf/"
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "first-contentful-paint": [
          "error",
          {
            "maxNumericValue": 3000
          }
        ],
        "interactive": [
          "error",
          {
            "maxNumericValue": 5200
          }
        ],
        "speed-index": [
          "error",
          {
            "maxNumericValue": 4300
          }
        ]
      }
    },
    "preset": "lighthouse:no-pwa"
  }
}